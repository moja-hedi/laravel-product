{
    "name": "moja-hedi/laravel-product",
    "description": "Product management package",
    "keywords": [
        "moja-hedi",
        "laravel",
        "product management",
        "product",
        "online shop system"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "aram",
            "email": "huseiny.aram@gmail.com"
        },
        {
            "name": "moja",
            "email": "m.abbaszadeh@apadanagroup.org"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "predis/predis": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "MojaHedi\\Product\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MojaHedi\\Product\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MojaHedi\\Product\\Providers\\ProductServiceProvider"
            ],
            "aliases": {
                "productsService": "MojaHedi\\Product\\Facades\\ProductServiceFacade"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
